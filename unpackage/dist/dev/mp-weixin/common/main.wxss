}
.cc-index-bar-anchor {
  z-index: 1;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 0 32rpx;
  color: #323233;
  font-weight: 500;
  font-size: 14px;
  line-height: 64rpx;
  background: #f5f5f5;
}
.cc-index-bar-anchor-active {
  color: #0081ff !important;
}
.cc-index-bar-anchor-fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  border-bottom: 1px solid #ebedf0;
}
.cc-index-bar-anchor-init-fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}
.cc-index-bar-anchor-active-index {
  height: 64rpx !important;
}
.cc-filed-cell-left-icon .cc-cell-left-icon {
